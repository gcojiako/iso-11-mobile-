        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chisara.IsoEleven"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4e0566e7-b059-4fc9-8a43-e90ee628f14e"
      }
    }
  }
}
